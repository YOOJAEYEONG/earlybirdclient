{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/essential",
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "vue",
    "import"
  ],
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "no-unused-vars": "warn"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "@",
            "./src"
          ]
        ]
      },
      "extensions": [
        ".js",
        ".vue"
      ]
    }
  }
}
