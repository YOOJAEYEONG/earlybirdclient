{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:vue/vue3-recommended",
    "plugin:import/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
//  "parser": "babel-eslint",
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaVersion": 2022,
    "ecmaFeatures": {
      "modules": true
    }
  },
  "plugins": [
    "vue",
    "import"
  ],
  "rules": {
    "quotes": [
      "warn",
      "double"
    ],
    "no-unused-vars": "warn",
    "max-len": "warn"
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "@",
            "./src"
          ]
        ]
      },
      "extensions": [
        ".js",
        ".vue"
      ]
    }
  }
}
